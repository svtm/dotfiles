*.font: xft:PragmataPro:pixelsize=16

Xft.dpi:                        96
Xft.antialias:                  1
Xft.autohint:                   1
Xft.hinting:                    1
Xft.hintstyle:                  hintslight
Xft.lcdfilter:                  lcdlight
Xft.rgba:                       rgb

URxvt*termName:                 rxvt-256color
URxvt.depth:                    32
URxvt*lineSpace:                1
URxvt.letterSpace:              -1
URxvt.iso14755:                 false
URxvt.iso14755_52:              false
URxvt*geometry:                 65x17
URxvt.scrollBar:                false
URxvt*loginShell:               true
URxvt.internalBorder:           25
URxvt.perl-ext:	 		default,matcher,resize-font,clipboard,keyboard-select
URxvt.url-launcher:             firefox
URxvt.matcher.button: 		1


URxvt.keysym.Control-Left:   \033[1;5D
URxvt.keysym.Control-Right:  \033[1;5C

! Clipboard
URxvt.keysym.Shift-Alt-V: eval:paste_clipboard
URxvt.keysym.Shift-Alt-C: eval:selection_to_clipboard

URxvt.keysym.C-Down: resize-font:smaller
URxvt.keysym.C-Up: resize-font:bigger

*.foreground: #FFFFFF
*.background: #252828
*.cursorColor: #5AF78E

*.color0: #252828
*.color1: #FF5C57
*.color2: #FFB536
*.color3: #F3F99D
*.color4: #5AF78E
*.color5: #9AEDFE
*.color6: #57C7FF
*.color7: #FFFFFF
